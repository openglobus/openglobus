{
  "include" : [
    "./src/og/**/*"
  ],
  "exclude" : [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "allowJs": true,
    "target": "es2016",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "out-tsc",
  }
}
